    "express-async-handler": "^1.2.0",
    "express-validator": "^6.14.0",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.2.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.18.0",
    "sequelize-cli": "^6.4.1",
    "uuid": "^8.3.2"
  },
